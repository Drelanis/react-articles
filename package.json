{
  "name": "production_project_code_course",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev:server": "node ./json-server/index.js",
    "start": "concurrently \"webpack serve --env port=3000\" \"npx typed-scss-modules src --watch --ignore **/*.main.scss\"",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "i18next:build": "i18next-resources-for-ts toc -i ./public/locales/en -o ./@types/resources.ts",
    "husky:install": "husky install",
    "lint:ts": "eslint .",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "visual:report": "npm-run-all visual:report:*",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
